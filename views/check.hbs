<button  type="submit"  class="flex-c-m stext-101 cl0 size-116 bg3 bor14 hov-btn3 p-lr-15 trans-04 pointer mt-3" id="placeOrder">
							Place Order
						</button>
              </div>
              
            </div>
            <!-- end right pannel section -->
          </div>
        </div>
        </form>
